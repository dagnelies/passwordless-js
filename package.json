{
  "name": "@passwordless-id/webauthn",
  "version": "0.0.10",
  "description": "A small wrapper around the webauthn protocol to make one's life easier.",
  "types": "dist/types/index.d.ts",
  "main": "dist/webauthn.min.js",
  "scripts": {
    "build:esbuild": "esbuild src/index.ts --platform=neutral --bundle --sourcemap --minify --target=es2022 --outfile=dist/webauthn.min.js",
    "build:esm": "tsc",
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build": "run-p build:*",
    "dev": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/passwordless-id/webauthn.git"
  },
  "keywords": [
    "authentication",
    "webauthn",
    "passkeys",
    "passwordless"
  ],
  "author": "Arnaud Dagnelies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/passwordless-id/webauthn/issues"
  },
  "homepage": "https://webauthn.passwordless.id",
  "devDependencies": {
    "esbuild": "^0.15.8",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.3"
  }
}
